; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Together in the Guts of the Machine"
run/main_scene="res://3d_ui.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"
Manager="*res://scenes/manager.tscn"

[dialogue_manager]

general/balloon_path="res://addons/dialogue_manager/example_balloon/edited_balloon.tscn"
general/states=["Globals", "Manager"]
general/wrap_lines=true

[display]

window/size/viewport_width=2400
window/size/viewport_height=1800

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogue_manager/plugin.cfg")

[internationalization]

locale/translations_pot_files=PackedStringArray("res://test.dialogue", "<null>")

[rendering]

textures/canvas_textures/default_texture_filter=0
